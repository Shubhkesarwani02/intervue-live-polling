{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/socket",
      "destination": "/api/socket"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SOCKET_URL": "@socket_url"
  },
  "functions": {
    "app/api/socket/route.ts": {
      "maxDuration": 30
    }
  }
}
